{
  "name": "greelogix/request-logger",
  "description": "Request logger with masking and UI for Laravel 10 and 11.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "request-logger",
    "logging",
    "http-logging",
    "api-logging",
    "request-tracking"
  ],
  "authors": [
    {
      "name": "GreeLogix",
      "email": "packages@greelogix.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "GreeLogix\\RequestLogger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "GreeLogix\\RequestLogger\\RequestLoggerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
